{
    "name": "isupozoy/web-skeleton",
    "description": "A powerful web skeleton built using Symfony.",
    "type": "project",
    "keywords": [
        "php",
        "web-skeleton",
        "symfony",
        "mvc-architecture",
        "psr-compliant",
        "secure-by-default"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "^2.6",
        "paragonie/ciphersweet": "^1.2",
        "symfony/options-resolver": "^4.1",
        "symfony/polyfill-apcu": "^1.8",
        "symfony/polyfill-ctype": "^1.8",
        "symfony/polyfill-mbstring": "^1.8",
        "symfony/polyfill-php70": "^1.8",
        "symfony/polyfill-php71": "^1.8",
        "symfony/polyfill-php72": "^1.8",
        "symfony/polyfill-php73": "^1.8",
        "symfony/polyfill-util": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Isupozoy\\": "./src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
