{
    "name": "isupozoy/crystal-app",
    "description": "Create crystal clear web applications with minimal configuration and easy to use tools.",
    "type": "project",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "^2.6",
        "paragonie/ciphersweet": "^1.2",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/debug": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/lock": "^4.1",
        "symfony/options-resolver": "^4.1",
        "symfony/polyfill-apcu": "^1.8",
        "symfony/polyfill-ctype": "^1.8",
        "symfony/polyfill-mbstring": "^1.8",
        "symfony/polyfill-php70": "^1.8",
        "symfony/polyfill-php71": "^1.8",
        "symfony/polyfill-php72": "^1.8",
        "symfony/polyfill-php73": "^1.8",
        "symfony/polyfill-util": "^1.8",
        "symfony/process": "^4.1",
        "symfony/routing": "^4.1",
        "symfony/templating": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Crystal\\App\\": "./crystalapp",
            "Crystal\\Controller\\": "./controllers"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
